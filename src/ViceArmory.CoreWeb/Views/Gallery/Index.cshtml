
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="gallery">
    <section id="gallery_section" class="gallery_section animate__slideInUp animate__animated">
        <div class="container">
            <div class="row">
                <div class="gallery_title page_title col-12 text-center">
                    <div class="page_title_box">
                        <h2 class="text-uppercase m-0">Gallery</h2>
                    </div>
                </div>
            </div>
            <div class="gallery_post row popup-gallery">
                <div class="post_box animate__animated animate__slideInUp animate__faster col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_1.png" class="gallrey_popup"><img src="~/Assets/gallery/image_1.png" alt="image_1"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__faster col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_2.png" class="gallrey_popup"><img src="~/Assets/gallery/image_2.png" alt="image_2"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__faster col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_3.png" class="gallrey_popup"><img src="~/Assets/gallery/image_3.png" alt="image_3"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__fast col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_4.png" class="gallrey_popup"><img src="~/Assets/gallery/image_4.png" alt="image_4"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__fast col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_5.png" class="gallrey_popup"><img src="~/Assets/gallery/image_5.png" alt="image_5"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__fast col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_6.png" class="gallrey_popup"><img src="~/Assets/gallery/image_6.png" alt="image_6"></a></div>
                    </div>
                </div>
                <div class="post_box animate__animated animate__slideInUp animate__fast col-lg-3 col-md-4 col-sm-12 col-12">
                    <div class="box_content">
                        <div class="content_img"><a href="~/Assets/gallery/image_7.png" class="gallrey_popup"><img src="~/Assets/gallery/image_7.png" alt="image_7"></a></div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="newsletter_section" class="newsletter_section animate__animated animate__faster animate__slideInUp">
        <div class="container">
            <div class="row">
                <div class="newsletter_box col-12">
                    <div class="box_bg text-center">
                        <h3>Subscribe to get the latest news, exclusive deals, and special pricing on featured items.</h3>
                        <form action="" method="post" class="d-flex flex-wrap align-items-center justify-content-center">
                            <div class="form_box input-box"><input type="email" class="form_box_input input" placeholder="Your email"></div>
                            <div class="form_box btn-box"><button type="submit" class="btn">Subscribe</button></div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
</section>

